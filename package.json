{
  "name": "abcp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "lint:scripts": "eslint \"src/**/*.{ts,tsx}\" --config .eslintrc.cjs",
    "lint:scripts:fix": "eslint \"src/**/*.{ts,tsx}\" --config .eslintrc.cjs --fix",
    "lint:types": "tsc",
    "prettier": "prettier \"src/**/*.{ts,tsx,css}\"",
    "prettier:check": "yarn prettier --list-different",
    "prettier:fix": "yarn prettier --write --list-different",
    "preview": "vite preview"
  },
  "dependencies": {
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-aliases": "^0.11.2"
  }
}
